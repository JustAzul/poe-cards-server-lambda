{
  "name": "poe.cards-lambda",
  "version": "1.1.0",
  "description": "lambda function for https://poe.cards",
  "scripts": {
    "build:aws:clean": "rimraf ./dist-aws",
    "build:aws": "npm run build && mkdirp ./dist-aws && cpy . ../dist-aws --cwd=dist && cpy ./package.json ./.env ./dist-aws",
    "build:clean": "rimraf ./dist",
    "lint": "eslint . --ext .ts --fix",
    "build": "tsc",
    "dev": "npm run build && concurrently \"tsc -w\" \"NODE_ENV=development nodemon dist/index.js\"",
    "prebuild": "npm run build:clean",
    "start": "npm run build && NODE_ENV=development node ./dist/index.js"
  },
  "author": "Diego (Azul) Ferreira",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.2",
    "dotenv": "^16.0.3",
    "firebase-admin": "^10.0.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.107",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "aws-lambda": "^1.0.7",
    "bestzip": "^2.2.1",
    "concurrently": "^7.4.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  }
}
