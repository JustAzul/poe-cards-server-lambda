{
  "name": "poe.cards-lambda",
  "version": "1.1.0",
  "description": "lambda function for https://poe.cards",
  "scripts": {
    "build:aws:clean": "rimraf ./dist-aws",
    "build:aws": "npm run build && mkdirp ./dist-aws && cpy . ../dist-aws --cwd=dist && cpy ./package.json ./.env ./dist-aws",
    "build:clean": "rimraf ./dist",
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "npm run build && concurrently \"npm run build:watch\" \"nodemon --delay 1 -r ./tsconfig-paths.js dist/index.js\"",
    "dev:fast": "nodemon --delay 1 -r ./tsconfig-paths.js dist/index.js",
    "lint": "eslint . --ext .ts --fix",
    "prebuild": "npm run build:clean",
    "start": "npm run build && node -r ./tsconfig-paths.js ./dist/index.js",
    "start:prod": "NODE_ENV=production node -r ./tsconfig-paths.js ./dist/index.js",
    "start:dev": "NODE_ENV=development node -r ./tsconfig-paths.js ./dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Diego (Azul) Ferreira",
  "license": "ISC",
  "dependencies": {
    "agentkeepalive": "^4.5.0",
    "axios": "^1.1.2",
    "dotenv": "^16.5.0",
    "firebase-admin": "^13.4.0",
    "micromatch": "^4.0.8",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.107",
    "@types/jest": "^29.5.5",
    "@types/node": "^24.0.3",
    "@types/reflect-metadata": "^0.1.0",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "aws-lambda": "^1.0.7",
    "bestzip": "^2.2.1",
    "concurrently": "^7.4.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-typescript-sort-keys": "^3.0.0",
    "fastify": "^4.29.1",
    "jest": "^29.7.0",
    "mkdirp": "^1.0.4",
    "nodemon": "^3.1.10",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "status-code-enum": "^1.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "uuid": "^9.0.1"
  }
}
